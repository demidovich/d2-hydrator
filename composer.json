{
    "name": "demidovich/d2-hydrator",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "hydrator",
        "ddd",
        "domain driven design"
    ],
    "autoload": {
        "psr-4": {
            "D2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Performance\\": "performance/"
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "^5.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
